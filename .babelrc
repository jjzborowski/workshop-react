{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry"
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/transform-runtime",
    "transform-class-properties",
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "common": "./src/common/",
        "components": "./src/components/"
      }
    }]
  ]
}
